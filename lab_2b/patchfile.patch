+	int rutime;
+	int stime;
+	int sums[3][3];
+	for (i = 0; i < 3; i++)
+		for (j = 0; j < 3; j++)
+			sums[i][j] = 0;
+	n = atoi(argv[1]);
+	i = n; //unimportant
+	int pid;
+	for (i = 0; i < 3 * n; i++) {
